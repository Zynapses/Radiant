{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "version": "1.0.0",
  "lastUpdated": "2026-01-25",
  "description": "Master manifest of ALL documentation files in RADIANT. AI agents MUST update relevant docs when making code changes.",
  
  "primaryDocs": {
    "description": "These documents MUST be checked on EVERY code change",
    "docs": [
      {
        "path": "CHANGELOG.md",
        "priority": "ALWAYS",
        "triggers": ["any_change"],
        "description": "Version history - update for ANY feature or fix"
      },
      {
        "path": "docs/RADIANT-ADMIN-GUIDE.md",
        "priority": "HIGH",
        "triggers": ["platform_feature", "admin_feature", "api_change", "cdk_change", "billing", "security", "models"],
        "description": "Platform admin documentation - tenants, billing, models, providers, security"
      },
      {
        "path": "docs/THINKTANK-ADMIN-GUIDE.md",
        "priority": "HIGH",
        "triggers": ["thinktank_admin", "user_rules", "delight", "brain_plans", "domains", "ego"],
        "description": "Think Tank admin features - user rules, delight system, brain plans"
      },
      {
        "path": "docs/THINKTANK-ADMIN-GUIDE-V2.md",
        "priority": "HIGH",
        "triggers": ["thinktank_admin", "user_rules", "delight", "brain_plans", "domains", "ego"],
        "description": "Think Tank admin guide v2 - consolidated version"
      },
      {
        "path": "docs/THINKTANK-USER-GUIDE.md",
        "priority": "HIGH",
        "triggers": ["user_facing", "ui_change", "thinktank_feature"],
        "description": "End-user documentation - how to use Think Tank"
      },
      {
        "path": "docs/STRATEGIC-VISION-MARKETING.md",
        "priority": "HIGH",
        "triggers": ["major_feature", "admin_guide_update"],
        "description": "Customer-facing marketing - auto-update when admin guides change"
      },
      {
        "path": "docs/ENGINEERING-IMPLEMENTATION-VISION.md",
        "priority": "HIGH",
        "triggers": ["architecture", "service", "lambda", "cdk", "technical"],
        "description": "Technical architecture and implementation details"
      },
      {
        "path": "docs/RADIANT-PLATFORM-ARCHITECTURE.md",
        "priority": "HIGH",
        "triggers": ["architecture", "cdk", "lambda", "api", "database"],
        "description": "Complete system architecture reference"
      }
    ]
  },
  
  "secondaryDocs": {
    "description": "Update when specific features are touched",
    "docs": [
      {
        "path": "docs/THINKTANK-MOATS.md",
        "triggers": ["competitive_advantage", "unique_feature"],
        "description": "Think Tank competitive differentiators"
      },
      {
        "path": "docs/RADIANT-MOATS.md",
        "triggers": ["platform_advantage", "unique_feature"],
        "description": "Platform competitive differentiators"
      },
      {
        "path": "docs/sections/SECTION-07-DATABASE-SCHEMA.md",
        "triggers": ["database", "migration", "table", "column"],
        "description": "Database schema documentation"
      },
      {
        "path": "docs/OPEN-SOURCE-LIBRARIES.md",
        "triggers": ["dependency", "npm_package", "library"],
        "description": "Open source library inventory"
      },
      {
        "path": "docs/UI-UX-PATTERNS.md",
        "triggers": ["ui_component", "design_pattern", "ux"],
        "description": "UI/UX pattern documentation"
      },
      {
        "path": "docs/ORCHESTRATION-METHODS.md",
        "triggers": ["orchestration", "workflow", "method"],
        "description": "AI orchestration method registry"
      },
      {
        "path": "docs/API_REFERENCE.md",
        "triggers": ["api_endpoint", "rest_api"],
        "description": "API reference documentation"
      },
      {
        "path": "docs/SERVICE-LAYER-GUIDE.md",
        "triggers": ["mcp", "a2a", "api_keys", "gateway", "service_layer", "cedar_policy", "interface_type"],
        "description": "Service layer documentation - MCP, A2A, API interfaces, gateway, Cedar policies"
      },
      {
        "path": "docs/CATO-COMPLETE-DOCUMENTATION.md",
        "triggers": ["cato", "safety", "genesis", "ethics"],
        "description": "Cato safety system documentation"
      },
      {
        "path": "docs/CONSCIOUSNESS-SERVICE.md",
        "triggers": ["consciousness", "ego", "affect"],
        "description": "Consciousness service documentation"
      },
      {
        "path": "docs/CORTEX-MEMORY-ADMIN-GUIDE.md",
        "triggers": ["cortex", "memory", "blackboard"],
        "description": "Cortex memory system documentation"
      },
      {
        "path": "docs/CURATOR-USER-GUIDE.md",
        "triggers": ["curator", "knowledge_graph", "verification", "golden_rules", "chain_of_custody"],
        "description": "Curator user guide - teaching AI, verification, overrides"
      },
      {
        "path": "docs/SWIFT-DEPLOYER-USER-GUIDE.md",
        "triggers": ["deployer", "swift_app", "deployment", "domain_config", "tier", "installation", "aws_setup"],
        "description": "Swift Deployer macOS app - installation, deployment, domain URLs, features"
      },
      {
        "path": "docs/UDS-ADMIN-GUIDE.md",
        "triggers": ["uds", "user_data", "conversation", "message", "upload", "audit", "erasure", "gdpr", "encryption"],
        "description": "User Data Service - tiered storage for conversations, uploads, audit, GDPR"
      },
      {
        "path": "docs/authentication/overview.md",
        "triggers": ["auth", "authentication", "signin", "sso", "mfa", "oauth"],
        "description": "Authentication system overview - all auth features, audiences, and navigation"
      },
      {
        "path": "docs/authentication/user-guide.md",
        "triggers": ["auth", "signin", "password", "passkey", "user_facing"],
        "description": "End-user authentication guide - sign-in, passwords, passkeys"
      },
      {
        "path": "docs/authentication/tenant-admin-guide.md",
        "triggers": ["auth", "sso", "mfa", "tenant_admin", "user_management"],
        "description": "Tenant admin auth guide - SSO config, MFA policies, user management"
      },
      {
        "path": "docs/authentication/platform-admin-guide.md",
        "triggers": ["auth", "cognito", "platform_admin", "security_policy"],
        "description": "Platform admin auth guide - Cognito management, global policies"
      },
      {
        "path": "docs/authentication/mfa-guide.md",
        "triggers": ["mfa", "totp", "2fa", "backup_codes", "trusted_devices"],
        "description": "MFA setup and usage guide - TOTP, backup codes, device trust"
      },
      {
        "path": "docs/authentication/oauth-guide.md",
        "triggers": ["oauth", "api_integration", "third_party", "tokens"],
        "description": "OAuth 2.0 developer guide - third-party app integration"
      },
      {
        "path": "docs/authentication/i18n-guide.md",
        "triggers": ["i18n", "localization", "language", "rtl", "cjk", "search"],
        "description": "Internationalization guide - 18 languages, RTL, CJK search"
      },
      {
        "path": "docs/authentication/troubleshooting.md",
        "triggers": ["auth", "signin", "mfa", "sso", "troubleshooting"],
        "description": "Authentication troubleshooting - common issues and solutions"
      },
      {
        "path": "docs/security/authentication-architecture.md",
        "triggers": ["security", "auth", "compliance", "threat_model", "cryptography"],
        "description": "Security architecture - threat models, compliance, crypto standards"
      },
      {
        "path": "docs/api/authentication-api.md",
        "triggers": ["api", "auth", "endpoint", "rest_api"],
        "description": "Authentication API reference - all auth endpoints"
      },
      {
        "path": "docs/api/search-api.md",
        "triggers": ["api", "search", "fts", "cjk", "multilang"],
        "description": "Multi-language search API - FTS, pg_bigm, CJK support"
      }
    ]
  },
  
